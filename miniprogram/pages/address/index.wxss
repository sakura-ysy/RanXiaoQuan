    margin-left: 5vw;
    margin-top: 2vh;
    margin-bottom: 2vh;
    background-color: rgb(29,29,119);
    border-radius: 30rpx;
    box-sizing: border-box;
    padding-top: 2vh;
}

.left_side {
    width: 80%;
    height: auto;
    display: inline-block;
    margin-bottom: 2vh;
}

.addr1>.left_side>.addr_single_pro {
    width: 100%;
    height: 3.5vh;
    margin-left: 5vw;
    font-size: 42rpx;
    font-weight: bold;
    color: white;
