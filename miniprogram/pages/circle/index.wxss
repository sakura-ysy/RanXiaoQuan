page {
    height: 100vh;
    width: 100vw;
    overflow: hidden;
    background-image: url("https://636c-cloud1-3gv71ub62b32f113-1312049560.tcb.qcloud.la/devImages/%E8%B5%84%E6%BA%90%20149%404x(1).png?sign=09100efa955127f335831f81df12917f&t=1656386549");
    background-size: 100%;
    background-repeat: no-repeat;
}

.back {
    width: 3vw;
    position: fixed;
    top: 7.5vh;
    left: 7vw;
}

.title {
    color: rgb(15,15,76);
    font-size: 60rpx;
    font-weight: 800;
    position: fixed;
    left: 20vw;
    top: 5.4vh;
}

.my {
    position: fixed;
    left: 5vw;
    top: 12vh;
    width: 24vw;
}

.search {
    position: fixed;
    right: 10vw;
    top: 11.5vh;
    width: 10vw;
}

.circle_all {
    position: fixed;
    top: 17vh;
    width: 100%;
    height: 80vh;
}

.single_post {
    width: 40%;
    margin-left: 5vw;
    margin-right: 5vw;
    margin-top: 1vh;
    display: inline-block;
}

.post_img {
    width: 100%;
    border-radius: 20rpx;
}

.line1 {
    height: auto;
    width: 100%;
}

.post_title {
    width: 60%;
    font-size: 37rpx;
    font-weight: 600;
    color: rgb(15,15,76);
    display: inline-block;
}

.heart {
    width: 20%;
    display: inline-block;
    transform: translate(0,10%);
}

.like_num {
    width: 15%;
    display: inline-block;
    transform: translate(0,-30%);
}

.user_icon {
    width: 20%;
    display: inline-block;
}

.user_name {
    width: 35%;
    display: inline-block;
    font-size: 30rpx;
    font-weight: 500;
    color: rgb(15,15,76);
    margin-left: 2%;
    transform: translate(0,-20%);
    overflow:hidden;
    text-overflow:ellipsis;
    white-space:nowrap;
}

.delete {
    width: 40%;
    transform: translate(0,0);
}